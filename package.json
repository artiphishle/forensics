{
  "name": "forensics",
  "type": "module",
  "engines": {
    "node": ">=22.14.0",
    "bun": ">=1.2.18",
    "npm": ">=10.9.2"
  },
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "docs:deploy": "bun --filter forensics-docs run deploy",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,mjs,cjs,html,css,scss,mdx}\"",
    "test": "bunx @artiphishle/testosterone",
    "test:cov": "c8 --reporter=text --reporter=text-summary tsx --test",
    "test:cov:html": "c8 --report-dir=./test/coverage --reporter=html tsx --test"
  },
  "dependencies": {
    "@artiphishle/testosterone": "^0.1.3",
    "cytoscape": "^3.31.2",
    "fp-ts": "^2.16.9",
    "lucide-react": "^0.487.0",
    "next": "15.3.3",
    "radix-ui": "^1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/cytoscape": "^3.21.9",
    "@types/cytoscape-klay": "^3.1.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.3",
    "tailwindcss-cli": "^0.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
}
